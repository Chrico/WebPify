{
  "name": "chrico/webpify",
  "description": "Generating automatically WebP-images and provide them via feature detection and lazy loading.",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian BrÃ¼ckner",
      "homepage": "https://www.chrico.info",
      "email": "chris@chrico.info",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7",
    "pimple/pimple": "^3.0"
  },
  "require-dev": {
    "php": ">=7",
    "brain/monkey": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "WebPify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebPify\\Tests\\": "tests/phpunit/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}