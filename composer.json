{
    "name": "chrico/webpify",
    "description": "Generating automatically WebP-images and provide them via feature detection and lazy loading.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian BrÃ¼ckner",
            "homepage": "https://www.chrico.info",
            "email": "chris@chrico.info",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7",
        "psr/container": "~1.0"
    },
    "require-dev": {
        "php": ">=7",
        "brain/monkey": "~2.1",
        "phpunit/phpunit": "~6.0",
        "inpsyde/php-coding-standards": "~0.13"
    },
    "autoload": {
        "psr-4": {
            "WebPify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebPify\\Tests\\": "tests/phpunit/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "suggest": {
        "ext-gd": "If you want to transform images into WebP-format, you can use the ext-gd extension",
        "ext-imagick": "If you want to transform into WebP-format, you can use the ext-imagick extension"
    }
}